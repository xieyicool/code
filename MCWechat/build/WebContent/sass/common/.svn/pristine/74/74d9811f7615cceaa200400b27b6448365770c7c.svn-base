.list-float{
    @include clearfix();
    >li{
        float:left;
        padding:0 $padding-large-horizontal 0 0;
        a{
            display:inline-block;
        }
    }
    
}
.list-inline-block{
    >li{
        display: inline-block;
        padding:0 $padding-large-horizontal 0 0;
    }
    a{
        display:inline-block;
    }
}
.list-decimal{
    list-style: decimal;
    margin-left: 18px;
    >li{
        margin-top: 10px;
    }
}
.list-dottor{
    list-style: disc;
    margin-left: 18px;
    >li{
        margin-top: 10px;
    }
}